{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.4",
    "guzzlehttp/guzzle": "^7.0",
    "moneyphp/money": "^3.3",
    "symfony/serializer-pack": "^1.0",
    "ext-json": "*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  }
}
